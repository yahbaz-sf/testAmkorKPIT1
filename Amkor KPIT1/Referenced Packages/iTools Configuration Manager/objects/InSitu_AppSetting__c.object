    <validationRules>
        <fullName>Large_Value_Required</fullName>
        <active>true</active>
        <description>Requires a value in the Large_Value__c field IF the Allow_Blank_Value__ is set to false.</description>
        <errorConditionFormula>(Allow_Blank_Value__c  ==  false )  &amp;&amp;  
(Value_Data_Type__c == 9 || Value_Data_Type__c == 10) &amp;&amp;
(LEN( Large_Value__c ) == 0)</errorConditionFormula>
        <errorDisplayField>Large_Value__c</errorDisplayField>
        <errorMessage>The Large Value cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scope_Indicator_Valid_Value</fullName>
        <active>true</active>
        <description>The Scope_Indicator__c must be either 1 (Global) or 5 (User Specific)</description>
        <errorConditionFormula>Scope_Indicator__c  != 1 &amp;&amp; Scope_Indicator__c  != 3 &amp;&amp; Scope_Indicator__c  !=  5</errorConditionFormula>
        <errorDisplayField>Scope_Indicator__c</errorDisplayField>
        <errorMessage>Invalid value for Scope Indicator.  Must be set to 1 if a Global setting, 3 if a Profile Specific setting or 5 if a User Specific setting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scope_ProfileID_Null</fullName>
        <active>true</active>
        <description>The Scope_ProfileID__c field must be null when the Scope_Indicator__c is set to global</description>
        <errorConditionFormula>Scope_Indicator__c  =  1 &amp;&amp; Scope_ProfileID__c  !=  null</errorConditionFormula>
        <errorDisplayField>Scope_ProfileID__c</errorDisplayField>
        <errorMessage>ProfileID cannot be set when the Scope Indicator is set to Global(1)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scope_ProfileID_Required</fullName>
        <active>true</active>
        <description>The Scope_ProfileID__c must be set when the Scope_Indicator is set to Profile (3)</description>
        <errorConditionFormula>Scope_Indicator__c == 3 &amp;&amp; Scope_ProfileID__c == null</errorConditionFormula>
        <errorDisplayField>Scope_ProfileID__c</errorDisplayField>
        <errorMessage>ProfileID is required when Scope Indicator is set to Profile Specific (3)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scope_User_Null</fullName>
        <active>true</active>
        <description>The Scope_User__c field must be null when the Acope_Indicator__c is set to global</description>
        <errorConditionFormula>Scope_Indicator__c  =  1 &amp;&amp; Scope_User__c  !=  null</errorConditionFormula>
        <errorDisplayField>Scope_User__c</errorDisplayField>
        <errorMessage>Scope user cannot be set when the Scope Indicator is set to Global(1)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scope_User_Required</fullName>
        <active>true</active>
        <description>The Scope_User__c must be set when the Scope_Indicator is set to User (5)</description>
        <errorConditionFormula>Scope_Indicator__c == 5 &amp;&amp; Scope_User__c == null</errorConditionFormula>
        <errorDisplayField>Scope_User__c</errorDisplayField>
        <errorMessage>Scope User is required when Scope Indicator is set to User Specific (5)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_Required</fullName>
        <active>true</active>
        <description>Requires a value in the Value__c field IF the Allow_Blank_Value__ is set to false.</description>
        <errorConditionFormula>(Allow_Blank_Value__c  ==  false )  &amp;&amp;  
(Value_Data_Type__c != 9 || Value_Data_Type__c != 10) &amp;&amp;
(LEN( Value__c ) == 0)</errorConditionFormula>
        <errorDisplayField>Value__c</errorDisplayField>
        <errorMessage>The Value cannot be blank.</errorMessage>
    </validationRules>
