public with sharing class ApexDefaultDriverHandler implements ApexDefaultDriver {
	public ApexDefaultDriverHandler() {}

	public void run(RFQItemWrapper wrapper, String objectId){
		if(wrapper.getAttribute('Handler_Model__c', objectId) != null && wrapper.getAttribute('Factory_Selected__c', objectId) != null && String.valueOf(wrapper.getAttribute('Handler__c', objectId)) != 'Consigned'){
			if(wrapper.getAttribute('Handler_Model_fml__c', objectId) != null){
				system.debug('Handler Model FML value: '+ String.valueOf(wrapper.getAttribute('Handler_Model_fml__c', objectId)).toUpperCase());
				Boolean available = valueSet.contains(String.valueOf(wrapper.getAttribute('Handler_Model_fml__c', objectId)).toUpperCase());
				//wrapper.specificRfqItem.put('Handler_Avaliable__c', available ? 'Yes' : 'No');
				wrapper.setAttributeToSpecificObject('Handler_Avaliable__c', available ? 'Yes' : 'No', objectId);
			}
		}else if(wrapper.getAttribute('Handler__c', objectId) != null && String.valueOf(wrapper.getAttribute('Handler__c', objectId)) == 'Consigned'){
			wrapper.setAttributeToSpecificObject('Handler_Avaliable__c', 'No', objectId);
		}else{
			wrapper.setAttributeToSpecificObject('Handler_Avaliable__c', null, objectId);
		}
	}

